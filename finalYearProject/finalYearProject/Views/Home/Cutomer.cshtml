            <style >
            .background {
                background-image: url('/images/cafe1.jpg');
                background-position: center;
                background-repeat: no-repeat;
                background-size: cover;
                height: 100vh;
                width: 100%;
            }

            .most-liked {
                position: relative;
                margin-top: 25px;
            }

            .video-container {
                position: absolute;
                top: 0;
                left: 0;
                height: 100%;
                width: 100%;
                z-index: -1;
            }

            .bg-video {
                height: 100%;
                width: 100%;
                object-fit: cover;
            }
            /* Make the image fully responsive */
            .carousel-inner img {
                width: 100%;
                height: 89%;
            }

            .cafe-card {
                position: relative;
                box-shadow: 5px 15px 50px #634832;
                max-width: 420px;
                transition: all .8s;
                overflow: hidden;
                cursor: pointer;
            }

            .rotate {
                perspective: 100rem;
            }

            .back {
                position: absolute;
                top: -100%;
                background: #fff;
                height: 100%;
                width: 100%;
                opacity: 0;
                user-select: none;
                pointer-events: none;
                transform: rotateY(180deg);
                transition: top .8s opacity .4s;
            }

            .back-content {
                position: absolute;
                top: 50%;
                left: 50%;
                transform: translate(-50%,-50%);
            }

            .rotate:hover .cafe-card {
                transform: rotateY(180deg);
            }

            .rotate:hover .back {
                opacity: 1;
                top: 0;
                user-select: auto;
                pointer-events: auto;
            }

            .back-content .btn {
                color: #000;
            }

            * {
                margin: 0;
                padding: 0;
                font-family: 'Josefin Sans',sans-serif;
            }

            .content-4 .nav {
                justify-content: center;
                margin-bottom: 5%;
            }

            .content-4 .nav-tabs {
                margin: 2px;
                border: none;
                border-bottom: 0px;
                text-align: center;
                cursor: pointer;
                background: #000;
            }

            .content-4 .tab-item {
                text-decoration: none;
                color: #dbc1ac;
            }

                .content-4 .tab-item i {
                    display: block;
                    font-size: 45px;
                    padding: 6px 27px 0px 27px;
                }

                .content-4 .tab-item span {
                    display: inline-block;
                    padding-bottom: 16px;
                    width: 100%;
                    padding-left: 5px;
                    padding-right: 5px;
                }

                .content-4 .tab-item.active {
                    color: #967259;
                }

            .tab-content {
                margin-bottom: 5%;
                transition: all ease-in;
            }
        </style>
